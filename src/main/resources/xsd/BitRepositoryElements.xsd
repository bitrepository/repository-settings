<!-- ************************************************************** -->
<!-- ****** USED TYPES IN ALPHABETIC ORDER                   ****** -->
<!-- ************************************************************** -->

  <xs:complexType name="Action_TYPE">
    <xs:annotation><xs:documentation xml:lang="en">
    Action that can be made on data in the bit repository
    </xs:documentation></xs:annotation>
    <xs:sequence>
      <xs:choice>
        <xs:element name="Access">
          <xs:annotation><xs:documentation xml:lang="en">
          Access action
          </xs:documentation></xs:annotation>
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:enumeration value="GET">
                <xs:annotation><xs:documentation xml:lang="en">
                Access via GET operation
                </xs:documentation></xs:annotation>
              </xs:enumeration>
              <xs:enumeration value="OTHER">
                <xs:annotation><xs:documentation xml:lang="en">
                Other forms of access
                </xs:documentation></xs:annotation>
              </xs:enumeration>
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="Create">
          <xs:annotation><xs:documentation xml:lang="en">
          Creation action
          </xs:documentation></xs:annotation>
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:enumeration value="INGEST">
                <xs:annotation><xs:documentation xml:lang="en">
                creation done via ingestion
                </xs:documentation></xs:annotation>
              </xs:enumeration>
              <xs:enumeration value="TRANSFER">
                <xs:annotation><xs:documentation xml:lang="en">
                creation done via transfer, e.g. after loss of data
                </xs:documentation></xs:annotation>
              </xs:enumeration>
              <xs:enumeration value="CORRECT">
                <xs:annotation><xs:documentation xml:lang="en">
                creation done as part of correction of fault data
                </xs:documentation></xs:annotation>
              </xs:enumeration>
              <xs:enumeration value="OTHER">
                <xs:annotation><xs:documentation xml:lang="en">
                creation done for other reasons
                </xs:documentation></xs:annotation>
              </xs:enumeration>
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="Endanger">
          <xs:annotation><xs:documentation xml:lang="en">
          Action possibly endagering the object
          </xs:documentation></xs:annotation>
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:enumeration value="UNKNOWNACCESS">
                <xs:annotation><xs:documentation xml:lang="en">
                Unknown access where object can have been involved
                </xs:documentation></xs:annotation>
              </xs:enumeration>
              <xs:enumeration value="FAILURE">
                <xs:annotation><xs:documentation xml:lang="en">
                Failure that can have involved object, e.g. general system failure with recovery actions
                </xs:documentation></xs:annotation>
              </xs:enumeration>
              <xs:enumeration value="OTHER">
                <xs:annotation><xs:documentation xml:lang="en">
                Other action that can have endagered object
                </xs:documentation></xs:annotation>
              </xs:enumeration>
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="IntegrityCheck">
          <xs:annotation><xs:documentation xml:lang="en">
          Action in connection with integrity check
          </xs:documentation></xs:annotation>
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:enumeration value="CHECKSUMSCHECK">
                <xs:annotation><xs:documentation xml:lang="en">
                Checksums check as part of integrity check
                </xs:documentation></xs:annotation>
              </xs:enumeration>
              <xs:enumeration value="REPLICACHECK">
                <xs:annotation><xs:documentation xml:lang="en">
                Check whether any/all replica exists as part of integrity check
                </xs:documentation></xs:annotation>
              </xs:enumeration>
              <xs:enumeration value="SALTCHECK">
                <xs:annotation><xs:documentation xml:lang="en">
                Check of checksum with salt as part of integrity check
                </xs:documentation></xs:annotation>
              </xs:enumeration>
              <xs:enumeration value="OTHER">
                <xs:annotation><xs:documentation xml:lang="en">
                Other checks as part of integrity check
                </xs:documentation></xs:annotation>
              </xs:enumeration>
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="Modify">
          <xs:annotation><xs:documentation xml:lang="en">
          Modification action
          </xs:documentation></xs:annotation>
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:enumeration value="COMPRESS">
                <xs:annotation><xs:documentation xml:lang="en">
                Modification in connection with compression
                </xs:documentation></xs:annotation>
              </xs:enumeration>
              <xs:enumeration value="CORRECT">
                <xs:annotation><xs:documentation xml:lang="en">
                Modification in form of minor internal correction, e.g. via RAID
                </xs:documentation></xs:annotation>
              </xs:enumeration>
              <xs:enumeration value="OTHER">
                <xs:annotation><xs:documentation xml:lang="en">
                Modification done for other reasons than the above
                </xs:documentation></xs:annotation>
              </xs:enumeration>
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="PhysicalMove">
          <xs:annotation><xs:documentation xml:lang="en">
          Physical move action
          </xs:documentation></xs:annotation>
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:enumeration value="MIGRATION">
                <xs:annotation><xs:documentation xml:lang="en">
                Move in connection with media migration
                </xs:documentation></xs:annotation>
              </xs:enumeration>
              <xs:enumeration value="OPERATION">
                <xs:annotation><xs:documentation xml:lang="en">
                Move in connection with internal operation, e.g. storage optimisation
                </xs:documentation></xs:annotation>
              </xs:enumeration>
              <xs:enumeration value="OTHER">
                <xs:annotation><xs:documentation xml:lang="en">
                Move done for other reasons than the above
                </xs:documentation></xs:annotation>
              </xs:enumeration>
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="Remove">
          <xs:annotation><xs:documentation xml:lang="en">
          Remove action
          </xs:documentation></xs:annotation>
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:enumeration value="TERMINATION">
                <xs:annotation><xs:documentation xml:lang="en">
                FinalResponse termination in remove of object
                </xs:documentation></xs:annotation>
              </xs:enumeration>
              <xs:enumeration value="PARTIAL">
                <xs:annotation><xs:documentation xml:lang="en">
                Partial removal with regret period
                </xs:documentation></xs:annotation>
              </xs:enumeration>
              <xs:enumeration value="OTHER">
                <xs:annotation><xs:documentation xml:lang="en">
                Other types of removal action
                </xs:documentation></xs:annotation>
              </xs:enumeration>
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
      </xs:choice>
      <xs:element name="ActionComment" type="xs:string" minOccurs="0" >
        <xs:annotation><xs:documentation xml:lang="en">
        Additional information about the action, e.g. details or specification of 'other' action
        </xs:documentation></xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="Actor_TYPE">
    <xs:annotation><xs:documentation xml:lang="en">
    Actor who can perform actions on data in the bit repository
    </xs:documentation></xs:annotation>
    <xs:sequence>
      <xs:element name="ActorID" type="ActorID_TYPE" >
        <xs:annotation><xs:documentation xml:lang="en">
        Identifier of the actor in question in the bit repository
        </xs:documentation></xs:annotation>
      </xs:element>
      <xs:element name="ActorName" type="xs:string" minOccurs="0">
        <xs:annotation><xs:documentation xml:lang="en">
        Name of the actor in question in the bit repository
        </xs:documentation></xs:annotation>
      </xs:element>
      <xs:element name="ActorType">
        <xs:annotation><xs:documentation xml:lang="en">
        Type of actor
        </xs:documentation></xs:annotation>
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:enumeration value="USER">
                <xs:annotation><xs:documentation xml:lang="en">
                User logged on e.g. coordination layer or pillar
                </xs:documentation></xs:annotation>
              </xs:enumeration>
              <xs:enumeration value="INTERNALUSER">
                <xs:annotation><xs:documentation xml:lang="en">
                Internal user
                </xs:documentation></xs:annotation>
              </xs:enumeration>
              <xs:enumeration value="INTERNALPROGRAM">
                <xs:annotation><xs:documentation xml:lang="en">
                Internal program
                </xs:documentation></xs:annotation>
              </xs:enumeration>
              <xs:enumeration value="PROCESS">
                <xs:annotation><xs:documentation xml:lang="en">
                Single File Processing operation
                </xs:documentation></xs:annotation>
              </xs:enumeration>
              <xs:enumeration value="MASSPROCESS">
                <xs:annotation><xs:documentation xml:lang="en">
                Mass processing operation
                </xs:documentation></xs:annotation>
              </xs:enumeration>
              <xs:enumeration value="OTHER">
                <xs:annotation><xs:documentation xml:lang="en">
                Something that cannot be covered by other alternatives.
                In this case ActorComment should be filled out
                </xs:documentation></xs:annotation>
              </xs:enumeration>
            </xs:restriction>
          </xs:simpleType>
      </xs:element>
      <xs:element name="ActorComment" type="xs:string" minOccurs="0" >
        <xs:annotation><xs:documentation xml:lang="en">
        Additional information about the actor
        </xs:documentation></xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:simpleType name="ActorID_TYPE">
    <xs:annotation><xs:documentation xml:lang="en">
    Unique identifiers for an actor doing actions in the Bit Repository
    </xs:documentation></xs:annotation>
    <xs:restriction base="xs:string"/>
  </xs:simpleType>

  <xs:simpleType name="Address_TYPE">
    <xs:annotation><xs:documentation xml:lang="en">
    Address for where data files can be found or placed
    </xs:documentation></xs:annotation>
    <xs:restriction base="xs:string"/>
  </xs:simpleType>

  <xs:simpleType name="ALARMCODE_TYPE">
    <xs:annotation><xs:documentation xml:lang="en">
    Error codes that are specific for Alarm messages
    </xs:documentation></xs:annotation>
    <xs:restriction base="xs:positiveInteger">
      <xs:enumeration value="541">
        <xs:annotation><xs:documentation xml:lang="en">
        General alarm
        </xs:documentation></xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="542">
        <xs:annotation><xs:documentation xml:lang="en">
        Unknown user access
        </xs:documentation></xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="AuditTrailInformation_TYPE">
    <xs:annotation><xs:documentation xml:lang="en">
    Audit Trail Information known to sender to be added to receiver audit trail.
    </xs:documentation></xs:annotation>
    <xs:restriction base="xs:string"/>
  </xs:simpleType>

  <xs:simpleType name="BitRepositoryCollectionID_TYPE">
    <xs:annotation><xs:documentation xml:lang="en">
    A unique identifier for bit repository collection which data belongs to.
    </xs:documentation></xs:annotation>
    <xs:restriction base="xs:string"/>
  </xs:simpleType>

  <xs:complexType name="ChecksumDataForChecksumSpec_TYPE">
    <xs:annotation><xs:documentation xml:lang="en">
    Checksum data belonging to certain checksum specification.
    </xs:documentation></xs:annotation>
    <xs:sequence>
      <xs:element name="FileID" type="FileID_TYPE" >
        <xs:annotation><xs:documentation xml:lang="en">
        Identifier of data the checksum is for
        </xs:documentation></xs:annotation>
      </xs:element>

      <xs:element name="ChecksumValue" type="ChecksumValue_TYPE" >
        <xs:annotation><xs:documentation xml:lang="en">
        The value of the checksum
        </xs:documentation></xs:annotation>
      </xs:element>

      <xs:element name="CalculationTimestamp" type="SystemDateTime_TYPE">
        <xs:annotation><xs:documentation xml:lang="en">
        The time when the checksum was last calculated
        (especially for ordinary checksums on off-line medias, there can be long intervals between
        calculations, - in such cases this information is crucial for alarming)
        </xs:documentation></xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="ChecksumDataForFile_TYPE">
    <xs:annotation><xs:documentation xml:lang="en">
    Checksum data belonging to a certain file ID.
    </xs:documentation></xs:annotation>
    <xs:sequence>
      <xs:element name="ChecksumSpec" type="ChecksumSpec_TYPE" >
        <xs:annotation><xs:documentation xml:lang="en">
        The type and possible salt for calculation of the checksum
        </xs:documentation></xs:annotation>
      </xs:element>

      <xs:element name="ChecksumValue" type="ChecksumValue_TYPE" >
        <xs:annotation><xs:documentation xml:lang="en">
        The value of the checksum
        </xs:documentation></xs:annotation>
      </xs:element>

      <xs:element name="CalculationTimestamp" type="SystemDateTime_TYPE">
        <xs:annotation><xs:documentation xml:lang="en">
        The time when the checksum was last calculated
        (especially for ordinary checksums on off-line medias, there can be long intervals between
        calculations, - in such cases this information is crucial for alarming)
        </xs:documentation></xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="ChecksumSpec_TYPE">
    <xs:sequence>
      <xs:element name="ChecksumType" type="ChecksumType_TYPE" >
        <xs:annotation><xs:documentation xml:lang="en">
        The checksum type
        </xs:documentation></xs:annotation>
      </xs:element>

      <xs:element name="ChecksumSalt" type="SaltParameter_TYPE" minOccurs="0" maxOccurs="1" >
        <xs:annotation><xs:documentation xml:lang="en">
        Optionally a salt parameter included in the checksum calculation
        </xs:documentation></xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:simpleType name="ChecksumType_TYPE">
    <xs:annotation><xs:documentation xml:lang="en">
    Could e.g. be MD5, SHA-1. the protocol, however, does not make restriction of
    which values that exists
    </xs:documentation></xs:annotation>
    <xs:restriction base="xs:string">
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="ChecksumValue_TYPE">
    <xs:restriction base="xs:string">
    </xs:restriction>
  </xs:simpleType>

  <xs:complexType name="Component_TYPE">
    <xs:annotation><xs:documentation xml:lang="en">
    Component of the bit repository
    </xs:documentation></xs:annotation>
    <xs:sequence>
      <xs:element name="ComponentID" type="ComponentID_TYPE" >
        <xs:annotation><xs:documentation xml:lang="en">
        Identifier of the component in question in the bit repository
        </xs:documentation></xs:annotation>
      </xs:element>
      <xs:element name="ComponentType">
        <xs:annotation><xs:documentation xml:lang="en">
        Component of the bit repository
        </xs:documentation></xs:annotation>
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:enumeration value="CLIENT">
                <xs:annotation><xs:documentation xml:lang="en">
                A Client component
                </xs:documentation></xs:annotation>
              </xs:enumeration>
              <xs:enumeration value="PILLAR">
                <xs:annotation><xs:documentation xml:lang="en">
                A Pillar component
                </xs:documentation></xs:annotation>
              </xs:enumeration>
              <xs:enumeration value="COORLAYER">
                <xs:annotation><xs:documentation xml:lang="en">
                A component in the coordination layer
                </xs:documentation></xs:annotation>
              </xs:enumeration>
            </xs:restriction>
          </xs:simpleType>
      </xs:element>
      <xs:element name="ComponentComment" type="xs:string" minOccurs="0" >
        <xs:annotation><xs:documentation xml:lang="en">
        Additional information about the component
        </xs:documentation></xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:simpleType name="ComponentID_TYPE">
    <xs:annotation><xs:documentation xml:lang="en">
    Unique identifiers for a component in the Bit Repository
    </xs:documentation></xs:annotation>
    <xs:restriction base="xs:string">
    </xs:restriction>
  </xs:simpleType>

  <xs:complexType name="ContributorStatus_TYPE">
    <xs:sequence>
      <xs:element name="ContributorStatusElements">
        <xs:complexType>
          <xs:sequence>
            <xs:element ref="NoOfItems" />
            <xs:element name="ContributorStatusElement" type="ContributorStatusElement_TYPE" minOccurs="1" maxOccurs="unbounded"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>

      <xs:element name="StatusTimestamp" type="SystemDateTime_TYPE">
        <xs:annotation><xs:documentation xml:lang="en">
        The time when the status was calculated
        </xs:documentation></xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="ContributorStatusElement_TYPE">
    <xs:choice>
      <xs:annotation><xs:documentation xml:lang="en">
      TBD: extend this later e.g. with status elements for ping, water level from pillars etc.
      </xs:documentation></xs:annotation>
      <xs:element name="StatusPlainText" type="xs:string" />
      <xs:element name="StatusPing" type="xs:string" />
    </xs:choice>
  </xs:complexType>

  <xs:simpleType name="CorrelationID_TYPE">
    <xs:restriction base="xs:string">
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="ERRORCODE_FINALRESPONSE_TYPE">
    <xs:annotation><xs:documentation xml:lang="en">
    Error codes that are specific for FinalResponse messages
    Numbers may later be adjusted to fit a standard
    </xs:documentation></xs:annotation>
    <xs:restriction base="xs:positiveInteger">
      <xs:enumeration value="581">
        <xs:annotation><xs:documentation xml:lang="en">
        Operation was not completed for unknown reason
        </xs:documentation></xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="ERRORCODE_GENERAL_TYPE">
    <xs:annotation><xs:documentation xml:lang="en">
    General error codes for response messages
    </xs:documentation></xs:annotation>
    <xs:restriction base="xs:positiveInteger">
      <xs:enumeration value="501">
        <xs:annotation><xs:documentation xml:lang="en">
        File with given FileID has NOT been found for a read request
        </xs:documentation></xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="502">
        <xs:annotation><xs:documentation xml:lang="en">
        Duplicate FileID in write request
        </xs:documentation></xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="561">
        <xs:annotation><xs:documentation xml:lang="en">
        Request was not understood
        </xs:documentation></xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="500">
        <xs:annotation><xs:documentation xml:lang="en">
        There are other reasons for failure
        text following error code must explain reason.
        This code should rarely be used, and use indicates that this type should be updated
        </xs:documentation></xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="FileID_TYPE">
    <xs:annotation><xs:documentation xml:lang="en">
    Unique identifiers for data in given service level agreement
    In a Get operation this will be the identifier for data requested
    In a Put operation this will be the identifier the data for later request of the data
    </xs:documentation></xs:annotation>
    <xs:restriction base="xs:string">
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="FinalResponseCode_TYPE">
    <xs:annotation><xs:documentation xml:lang="en">
    Type with all final response codes for final response messages in the bit repository protocol
    </xs:documentation></xs:annotation>
    <xs:union memberTypes="FinalResponseCodePositive_TYPE ERRORCODE_GENERAL_TYPE ERRORCODE_FINALRESPONSE_TYPE"/>
  </xs:simpleType>

  <xs:simpleType name="FinalResponseCodePositive_TYPE">
    <xs:restriction base="xs:positiveInteger">
      <xs:enumeration value="480">
        <xs:annotation><xs:documentation xml:lang="en">
        Message request has been completed successfully
        </xs:documentation></xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="IdentifyResponseCode_TYPE">
    <xs:annotation><xs:documentation xml:lang="en">
    Type of all codes for Identify Response messages.
    </xs:documentation></xs:annotation>
    <xs:union memberTypes="IdentifyResponseCodePositive_TYPE ERRORCODE_GENERAL_TYPE"/>
  </xs:simpleType>

  <xs:simpleType name="IdentifyResponseCodePositive_TYPE">
    <xs:restriction base="xs:positiveInteger">
      <xs:enumeration value="440">
        <xs:annotation><xs:documentation xml:lang="en">
        Message request has been received and operation request is expected to be successful
        </xs:documentation></xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="PillarID_TYPE">
    <xs:restriction base="xs:string">
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="PriorityCode_TYPE">
    <xs:annotation><xs:documentation xml:lang="en">
    Global priority codes for the system, which indicate e.g. how and how fast alarms must be treated
    </xs:documentation></xs:annotation>
    <xs:restriction base="xs:positiveInteger">
      <xs:enumeration value="1">
        <xs:annotation><xs:documentation xml:lang="en">
        Alarm requires manual checks. Notification should be sent.
        </xs:documentation></xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="2">
        <xs:annotation><xs:documentation xml:lang="en">
        Integrity Client Alarm: Missing IDs.
        </xs:documentation></xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="3">
        <xs:annotation><xs:documentation xml:lang="en">
        Integrity Client Alarm: Checksum errors.
        </xs:documentation></xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="4">
        <xs:annotation><xs:documentation xml:lang="en">
        Other. Notification should be sent. Alarmtext should explain what the alarm consists of.
        This code should rarely be used, and use indicates that this type should be updated.
        </xs:documentation></xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="ProgressResponseCode_TYPE">
    <xs:annotation><xs:documentation xml:lang="en">
    Type with all progress response codes for progress response message in the bit repository protocol
    </xs:documentation></xs:annotation>
    <xs:restriction base="xs:positiveInteger">
      <xs:enumeration value="460">
        <xs:annotation><xs:documentation xml:lang="en">
        Message request has been received and is expected to be met successfully
        </xs:documentation></xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="461">
        <xs:annotation><xs:documentation xml:lang="en">
        Data from a PutFile request has been buffered successfully
        </xs:documentation></xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="462">
        <xs:annotation><xs:documentation xml:lang="en">
        Data for a FileID has been found for some sort of Get request
        </xs:documentation></xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="QueueTopicName_TYPE">
    <xs:restriction base="xs:string">
    </xs:restriction>
  </xs:simpleType>

  <xs:complexType name="Risk_TYPE">
    <xs:annotation><xs:documentation xml:lang="en">
      Indicates Risk and Risk Area for Alarm if not treated
    </xs:documentation></xs:annotation>
    <xs:sequence>
      <xs:element name="RiskProbabilityScore" type="RiskProbabilityScore_TYPE">
        <xs:annotation><xs:documentation xml:lang="en">
          Probability of specified scenario becoming reality, if alarm is not treated.
        </xs:documentation></xs:annotation>
      </xs:element>

      <xs:element name="RiskImpactScore" type="RiskImpactScore_TYPE">
        <xs:annotation><xs:documentation xml:lang="en">
          Potential impact of specified scenario becoming reality, if alarm is not treated.
        </xs:documentation></xs:annotation>
      </xs:element>

      <xs:element name="RiskArea" type="RiskArea_TYPE">
        <xs:annotation><xs:documentation xml:lang="en">
          Global risk code for likelyhood for certain risks to become reality, if alarm is not treated.
          Known risk areas are: availability, bit safety, confidentiality
        </xs:documentation></xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:simpleType name="RiskArea_TYPE">
    <xs:restriction base="xs:positiveInteger">
      <xs:enumeration value="1">
        <xs:annotation><xs:documentation xml:lang="en">
          Risk Area availability
        </xs:documentation></xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="2">
        <xs:annotation><xs:documentation xml:lang="en">
          Risk Area bit safety
        </xs:documentation></xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="3">
        <xs:annotation><xs:documentation xml:lang="en">
          Risk Area confidentiality
        </xs:documentation></xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="RiskImpactScore_TYPE">
    <xs:restriction base="xs:nonNegativeInteger">
      <xs:enumeration value="0">
        <xs:annotation><xs:documentation xml:lang="en">
          Zero impact, results in zero loss of digital object authenticity and understandability
        </xs:documentation></xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="1">
        <xs:annotation><xs:documentation xml:lang="en">
          Negligible impact, results in isolated but fully recoverable loss of
          digital object authenticity and understandability
        </xs:documentation></xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="2">
        <xs:annotation><xs:documentation xml:lang="en">
          Superficial impact, results in widespread but fully recoverable
          loss of digital object authenticity and understandability
        </xs:documentation></xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="3">
        <xs:annotation><xs:documentation xml:lang="en">
          Medium impact, results in total but fully recoverable loss of
          digital object authenticity and understandability
        </xs:documentation></xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="4">
        <xs:annotation><xs:documentation xml:lang="en">
          High impact, results in isolated loss, including unrecoverable loss
          of digital object authenticity and understandability
        </xs:documentation></xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="5">
        <xs:annotation><xs:documentation xml:lang="en">
          Considerable impact, results in widespread loss, including unrecoverable loss or loss that is
          recoverable only by third party of digital object authenticity and understandability
        </xs:documentation></xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="6">
        <xs:annotation><xs:documentation xml:lang="en">
          Cataclysmic impact, results in total and unrecoverable loss of
          digital object authenticity and understandability
        </xs:documentation></xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="RiskProbabilityScore_TYPE">
    <xs:restriction base="xs:positiveInteger">
      <xs:enumeration value="1">
        <xs:annotation><xs:documentation xml:lang="en">
        Minimal probability, occurs once every 100 years or more
        </xs:documentation></xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="2">
        <xs:annotation><xs:documentation xml:lang="en">
        Very low probability, occurs once every 10 years
        </xs:documentation></xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="3">
        <xs:annotation><xs:documentation xml:lang="en">
        Low probability, occurs once every 5 years
        </xs:documentation></xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="4">
        <xs:annotation><xs:documentation xml:lang="en">
        Medium probability, occurs once every year
        </xs:documentation></xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="5">
        <xs:annotation><xs:documentation xml:lang="en">
        High probability, occurs once every month
        </xs:documentation></xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="6">
        <xs:annotation><xs:documentation xml:lang="en">
        Very high probability, occurs more than once every month
        </xs:documentation></xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="SaltParameter_TYPE">
    <xs:annotation><xs:documentation xml:lang="en">
    An extra salt to be added in the checksum calculation.
    If this parameter is given, it follows that the checksum calculation must include the salt
    </xs:documentation></xs:annotation>
    <xs:restriction base="xs:string">
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="SystemDateTime_TYPE">
    <xs:annotation><xs:documentation xml:lang="en">
    General type for how date/time is specifed in the bit repository
    </xs:documentation></xs:annotation>
    <xs:restriction base="xs:dateTime">
    </xs:restriction>
  </xs:simpleType>

  <xs:complexType name="TimeMeasure_TYPE">
    <xs:annotation><xs:documentation xml:lang="en">
    Indicates time measure from a point in time to another.
    In first version this will be in miliseconds, later it will be consisting of time and unit according to a standard
    </xs:documentation></xs:annotation>
    <xs:sequence>
      <xs:element name="TimeMeasureUnit" >
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="MILLISECONDS">
              <xs:annotation><xs:documentation xml:lang="en">
              Measure is specified in milli seconds
              </xs:documentation></xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="HOURS">
              <xs:annotation><xs:documentation xml:lang="en">
              Measure is specified in hours
              </xs:documentation></xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>

      <xs:element name="TimeMeasureValue" type="xs:nonNegativeInteger" >
        <xs:annotation><xs:documentation xml:lang="en">
        number of units
        </xs:documentation></xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

</xs:schema>
